:root{
--bs-primary: #{{ v_color_primary }};
--bs-primary-rgb: {{ v_color_primary  | hex2rgb3 }};
--bs-secondary: #{{ v_color_secondary }};
--bs-secondary-rgb: {{ v_color_secondary  | hex2rgb3 }};
--bs-success: #{{ v_color_success }};
--bs-success-rgb: {{ v_color_success  | hex2rgb3 }};
--bs-info: #{{ v_color_info }};
--bs-info-rgb: {{ v_color_info  | hex2rgb3 }};
--bs-warning: #{{ v_color_warning }};
--bs-warning-rgb: {{ v_color_warning  | hex2rgb3 }};
--bs-danger: #{{ v_color_danger }};
