.feather-bg-img{background-position:center center;background-size:cover}
.pricing-box{box-shadow:0 3px 12px rgba({{ v_color_pricing_shadow |hex2rgb3 }},.06);margin-bottom:30px;position:relative;overflow:hidden;background-color:var(--bs-secondary-bg)}
.pricing-box .pricing-icon-bg{background:rgba({{ v_color_primary |hex2rgb3 }},.12);color:#{{ v_color_primary }};display:block;height:80px;width:80px;line-height:100px;margin:0 auto;border-radius:20% 20% 4% 20%/20% 20% 4% 20%;box-shadow:0 0 0 7px rgba({{ v_color_primary |hex2rgb3 }},.03);transition:all .5s}
.pricing-box .pricing-item li{padding:10px 0}
.pricing-box .pr-btn{position:relative;z-index:1}
.pricing-box:hover .pricing-icon-bg,.pricing-box.active .pricing-icon-bg{background-color:#{{ v_color_primary }};box-shadow:0 0 0 7px rgba({{ v_color_primary |hex2rgb3 }},.06);color: #{{ v_color_white }};border-radius:4% 20% 20% 20%/4% 20% 20% 20%}
[data-bs-theme=dark] .pricing-box{background-color:var(--bs-white) !important}
.form-control:focus{box-shadow:none;background-color:#{{ v_color_light }};border-color:#{{ v_color_gray_700 }}}
textarea.form-control{height:auto !important}
.custom-form .form-group{margin-bottom:1rem}
.custom-form label{margin-bottom:10px}
.custom-form .alert-warning{padding:10px}
.form-control{padding:8px 10px;font-size:14px;height:40px;background-color:rgba({{ v_color_dark |hex2rgb3 }},.01);border:1px solid rgba({{ v_color_muted |hex2rgb3 }},.3)}
.form-control::-moz-placeholder{color:#{{ v_color_text_placeholder }};font-size:14px;font-weight:bold;}
.form-control::placeholder{color:#{{ v_color_text_placeholder }};font-size:14px;font-weight:bold;}
.contact-loader{display:none}
#success_page{background-color:rgba({{ v_color_success |hex2rgb3 }},.25);padding:10px 20px;border-radius:4px}
[data-bs-theme=dark] .form-control:focus{background-color:var(--bs-secondary-bg);border:1px solid rgba({{ v_color_muted |hex2rgb3 }},.3)}
.footer{background:var(--bs-footer-bg);padding:70px 0 40px}
.footer .footer-social-icon-content .footer-social-icon{color:rgba({{ v_color_white |hex2rgb3 }},.5);transition:all .5s}
.footer .footer-social-icon-content .footer-social-icon:hover{color: #{{ v_color_white }}}
.footer .footer-sub-menu li{padding:6px 0}
.footer .footer-sub-menu li .footer-link{color:rgba({{ v_color_white |hex2rgb3 }},.6);font-size:15px;transition:all .5s}
.footer .footer-sub-menu li .footer-link:hover{color: #{{ v_color_white }};margin-left:5px}
.footer-alt{background:var(--bs-footer-alt-bg)}
.modal .modal-dialog .modal-content .modal-body{padding:32px}
.modal .modal-dialog .modal-content .modal-body .social-login{position:relative;background: #{{ v_color_white }};padding:0 12px;display:inline-block}
.modal .modal-dialog .modal-content .modal-body .login-border{border:1px solid #{{ v_color_form_bg }};position:absolute;left:0;right:0;top:16px}
.modal .modal-dialog .modal-content .modal-body .login-social-icon{height:45px;width:45px;display:block;color: #{{ v_color_white }};line-height:45px;border-radius:50%;transition:all .5s}
.modal .form-group{margin-bottom:1rem}
.modal .form-group label{margin-bottom:10px}
.login-page{max-width:420px}
.login-form .login-social-icon.icon-primary{background-color:#{{ v_color_primary }}}
.login-form .login-social-icon.icon-primary:hover{box-shadow:0 0 0 5px rgba({{ v_color_primary |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-secondary{background-color:#{{ v_color_secondary }}}
.login-form .login-social-icon.icon-secondary:hover{box-shadow:0 0 0 5px rgba({{ v_color_secondary |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-success{background-color:#{{ v_color_success }}}
.login-form .login-social-icon.icon-success:hover{box-shadow:0 0 0 5px rgba({{ v_color_success |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-info{background-color:#{{ v_color_info }}}
.login-form .login-social-icon.icon-info:hover{box-shadow:0 0 0 5px rgba({{ v_color_info |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-warning{background-color:#{{ v_color_warning }}}
.login-form .login-social-icon.icon-warning:hover{box-shadow:0 0 0 5px rgba({{ v_color_warning |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-danger{background-color:#{{ v_color_danger }}}
.login-form .login-social-icon.icon-danger:hover{box-shadow:0 0 0 5px rgba({{ v_color_danger |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-dark{background-color:#{{ v_color_dark }}}
.login-form .login-social-icon.icon-dark:hover{box-shadow:0 0 0 5px rgba({{ v_color_dark |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-muted{background-color:#{{ v_color_muted }}}
.login-form .login-social-icon.icon-muted:hover{box-shadow:0 0 0 5px rgba({{ v_color_muted |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-light{background-color:#{{ v_color_light }}}
.login-form .login-social-icon.icon-light:hover{box-shadow:0 0 0 5px rgba({{ v_color_light |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-body-bg{background-color:#{{ v_color_white }}}
.login-form .login-social-icon.icon-body-bg:hover{box-shadow:0 0 0 5px rgba({{ v_color_white |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-black{background-color:#000}
.login-form .login-social-icon.icon-black:hover{box-shadow:0 0 0 5px rgba({{ v_color_black |hex2rgb3 }},.16)}
.login-form .login-social-icon.icon-white{background-color: #{{ v_color_white }}}
.login-form .login-social-icon.icon-white:hover{box-shadow:0 0 0 5px rgba({{ v_color_white |hex2rgb3 }},.16)}
