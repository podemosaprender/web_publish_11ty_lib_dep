a{text-decoration:none !important;outline:none}
body{font-family:"{{ v_body_font }}",{{v_body_font_variant}};color:var(--bs-dark);font-weight:500}
p{line-height:1.8}
h1,h2,h3,h4,h5,h6{font-family:"{{ v_title_font }}",{{ v_title_font_variant }};font-weight:700}
.row>*{position:relative}
.navbar{padding-right:0px !important}
.nav-item .nav-link{display:inline-block;color:#{{ v_color_dark }};text-decoration:none;font-family:"{{ v_title_font }}",{{ v_title_font_variant }}}
.navbar-custom{padding:15px 0px;width:100%;border-radius:0px;z-index:999;margin-bottom:0px;transition:all .5s ease-in-out;background-color:rgba( {{ v_color_black |hex2rgb3 }},0)}
body{ padding-top: {{ v_logo_height_px * 2 }}px; }
img.logo{height: {{ v_logo_height_px }}px; }
.navbar-custom .logo .logo-dark{display:none}
.navbar-custom .logo .logo-light{display:inline-block}
.navbar-custom .navbar-nav li a{line-height:26px;color:rgba({{ v_color_dark |hex2rgb3 }},.7);font-size:15px;font-weight:600;transition:all .3s;background-color:rgba({{ v_color_black |hex2rgb3 }},0) !important;padding:6px 0;margin:0 20px}
.navbar-custom .navbar-nav li .nav-link{padding-right:0px;padding-left:0px}
.navbar-custom .navbar-nav li.active a,.navbar-custom .navbar-nav li a:hover,.navbar-custom .navbar-nav li a:active{color:#{{ v_color_primary }} !important}
.navbar-custom .navbar-toggles{padding:.25rem .75rem;font-size:18px;background:0 0;border:1px solid rgba({{ v_color_black |hex2rgb3 }},0);color: #{{ v_color_white }};outline:0}
.navbar-custom .logo .logo-dark{display:inline-block}
.navbar-custom .logo .logo-light{display:none}
.navbar-toggler{font-size:24px;margin-top:5px;margin-bottom:0px;color: #{{ v_color_white }}}
.navbar-toggler:focus{box-shadow:none !important}
.nav .open>a,.nav .open>a:focus,.nav .open>a:hover{background-color:rgba({{ v_color_black |hex2rgb3 }},0);border-color:#{{ v_color_primary }}}
.menu-toggle{padding:4.5px 10px !important}
.menu-toggle span{line-height:27px}
.navbar-light .navbar-nav li a{color:rgba({{ v_color_black |hex2rgb3 }},.6) !important}
.navbar-light .navbar-nav li.active a,.navbar-light .navbar-nav li a:hover,.navbar-light .navbar-nav li a:active{color: #{{ v_color_white }} !important}
.navbar-light .logo .logo-dark{display:none}
.navbar-light .logo .logo-light{display:inline-block}
.navbar-light .nav-link:after{background:rgba({{ v_color_black |hex2rgb3 }},0)}
.navbar-light .navbar-toggler{border-color:rgba({{ v_color_black |hex2rgb3 }},0)}
.navbar-light .navbar-nav li.active a,.navbar-light .navbar-nav li a:hover,.navbar-light .navbar-nav li a:active{color: #{{ v_color_white }} !important}
.nav-sticky.navbar-custom{margin-top:0px;background-color:var(--bs-body-bg);box-shadow:0 0 10px 0 rgba({{ v_color_black |hex2rgb3 }},.06);color:#000 !important}
.nav-sticky.navbar-custom .navbar-toggles{padding:.25rem .75rem;border:1px solid rgba({{ v_color_black |hex2rgb3 }},0);outline:0}
.nav-sticky.navbar-custom .navbar-nav{margin-top:0px}
.nav-sticky.navbar-custom .navbar-nav li a{color:#{{ v_color_navbar_link }} !important}
.nav-sticky.navbar-custom .navbar-nav li.active a,.nav-sticky.navbar-custom .navbar-nav li a:hover,.nav-sticky.navbar-custom .navbar-nav li a:active{color:#{{ v_color_primary }} !important}
.nav-sticky .logo .logo-dark{display:inline-block !important}
.nav-sticky .logo .logo-light{display:none !important}
.navbar-toggler:focus{outline:none}
@media(min-width: 200px)and (max-width: 991px){.navbar-custom{margin-top:0px;background-color:var(--bs-body-bg);box-shadow:0 3px 10px rgba({{ v_color_black |hex2rgb3 }},.08);color: #{{ v_color_white }} !important}
.navbar-custom .navbar-nav{margin-top:0px}
.navbar-custom .navbar-nav li a{transition:all .4s;color:#{{ v_color_muted }} !important;margin:0px}
.navbar-custom .navbar-nav li.active a{border-color:rgba({{ v_color_black |hex2rgb3 }},0)}
.navbar-custom .navbar-nav li.active a,.navbar-custom .navbar-nav li a:hover,.navbar-custom .navbar-nav li a:active{color:#{{ v_color_primary }} !important}
.navbar-custom>.container{width:90%}
.navbar-custom .logo .logo-dark{display:inline-block !important}
.navbar-custom .logo .logo-light{display:none !important}
.navbar-toggler{font-size:24px;margin-top:0px;margin-bottom:0px;color:#{{ v_color_dark }};border:1px solid rgba({{ v_color_black |hex2rgb3 }},0)}
.navbar-collapse{padding:0 20px}
.navbar-light .nav-item .nav-link:after{background:#{{ v_color_dark }}}
}
@media(min-width: 1200px){.container{max-width:1140px !important}
}
