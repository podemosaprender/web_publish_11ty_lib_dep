{# Bootstrap implementation of components #}

{% macro navbar(links="",search=true) %}
	{% if links == "" %}{% set links = cfg.navbar.links %}{% endif %}
	<nav class="navbar navbar-expand-md navbar-dark bg-dark mb-4">
		<div class="container-fluid">
			<a class="navbar-brand" href="{{ cfg.url }}">
				<img src="/img/logo.svg" alt="" width="30" height="24" class="d-inline-block align-text-top">
				{{ cfg.brand	}}
			</a>
			<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"></span>
			</button>
			<div class="collapse navbar-collapse" id="navbarCollapse">
				<ul class="navbar-nav me-auto mb-2 mb-md-0">
					{% for url,text in links %}
						<li class="nav-item">
							<a class="nav-link{% if url == page.url %} active{% endif %}" aria-current="page" href="{{ url }}">{{ text }}</a>
						</li>
					{% endfor %}
				</ul>
				{% if search %}
					<form class="d-flex" action="/search/" method="GET">
						<input name="q" class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
						<button class="btn btn-outline-success" type="submit">Search</button>
					</form>
				{% endif %}
			</div>
		</div>
	</nav>
{% endmacro %}

{# CATEGORY: typography #}

{% macro typo_display(cls='',level=1,text='') %}
	<p class="display-{{ level }} {{cls}}">{% if caller %}{{ caller() }}{% endif %}{{ text }}</p>
{% endmacro %}

{% macro typo_lead(cls='',text='') %}
	<p class="lead">{% if caller %}{{ caller() }}{% endif %}{{ text }}</p>
{% endmacro %}

{% macro typography_4(cls='') %}
	<p>You can use the mark tag to <mark>highlight</mark> text.</p>
	<p><del>This line of text is meant to be treated as deleted text.</del></p>
	<p><s>This line of text is meant to be treated as no longer accurate.</s></p>
	<p><ins>This line of text is meant to be treated as an addition to the document.</ins></p>
	<p><u>This line of text will render as underlined.</u></p>
	<p><small>This line of text is meant to be treated as fine print.</small></p>
	<p><strong>This line rendered as bold text.</strong></p>
	<p><em>This line rendered as italicized text.</em></p>
{% endmacro %}{# typography_4 #}

{% macro bquote(cls='',text='',author='',source='') %}
	<blockquote class="blockquote {{ cls }}">
		{% if text != '' %}<p>{{ text }}</p>{% endif %}
		{% if caller %}{{ caller() }}{% endif %}
		<footer class="blockquote-footer">{{ author}} <cite title="{{ source }}">{{ source }}</cite></footer>
	</blockquote>
{% endmacro %}

{% macro list_nostyle(cls='', items=[]) %}
	<ul class="list-unstyled {{ cls }}">
		{% for it in items %}
			<li>{{ it }}</li>
		{% endfor %}
		{% if caller %}{{ caller() }}{% endif %}
	</ul>
{% endmacro %}

{% macro list_inline(cls='', items=[]) %}
	<ul class="list-inline {{ cls }}">
		{% for it in items %}
			<li class="list-inline-item">{{ it }}</li>
		{% endfor %}
		{% if caller %}{{ caller() }}{% endif %}
	</ul>
{% endmacro %}


{# CATEGORY: images #}

{% macro img_resp(cls='',src='',label='Image') %}
	<img src="{{src}}" class="img-fluid {{ cls }}" width="100%" height="250" role="img" aria-label="{{label}}" preserveAspectRatio="xMidYMid slice" focusable="false" />
{% endmacro %}

{% macro img_thumb(cls='',src='',label='Image') %}) %}
	<svg class="img-thumbnail {{cls}}" width="200" height="200" role="img" aria-label="{{label}}" preserveAspectRatio="xMidYMid slice" focusable="false" />
{% endmacro %}

{# CATEGORY: figures #}

{% macro img_fig(cls='', label='', src='') %}
	<figure class="figure {{cls}}">
		<img class="figure-img img-fluid rounded" src="{{src}}" width="400" height="300" role="img" aria-label="{{label}}" preserveAspectRatio="xMidYMid slice" focusable="false"/>
		<figcaption class="figure-caption">{{label}}</figcaption>
	</figure>
{% endmacro %}{# figures_1 #}

{# CATEGORY: carousel #}

{% macro carousel(cls='',id='carousel',slides={}) %}
	<div id="{{id}}" class="carousel slide {{cls}}" data-bs-ride="carousel">
		<div class="carousel-indicators">
			{% for lbl,data in slides %}
				<button type="button" data-bs-target="#{{id}}" data-bs-slide-to="{{ loop.index0 }}" {% if loop.first %}class="active" aria-current="true"{% endif %} aria-label="{{lbl}}"></button>
			{% endfor %}
		</div>
		<div class="carousel-inner">
			{% for lbl,data in slides %}
				<div class="carousel-item {% if loop.first %}active{%endif%}">
				<img class="d-block w-100" width="800" height="400" role="img" aria-label="{{lbl}}" preserveAspectRatio="xMidYMid slice" focusable="false"/>
				<div class="carousel-caption d-none d-md-block">
					{{ data.html | safe }}
				</div>
			</div>
			{% endfor %}
		</div>
		<button class="carousel-control-prev" type="button" data-bs-target="#{{loop.index0}}"  data-bs-slide="prev">
			<span class="carousel-control-prev-icon" aria-hidden="true"></span>
			<span class="visually-hidden">Previous</span>
		</button>
		<button class="carousel-control-next" type="button" data-bs-target="#{{loop.index0}}"  data-bs-slide="next">
			<span class="carousel-control-next-icon" aria-hidden="true"></span>
			<span class="visually-hidden">Next</span>
		</button>
	</div>
{% endmacro %}{# carousel_1 #}

{# CATEGORY: buttons #}

{% macro btn(cls='',attr='',type='primary',label='',outline=false,sz=false) %}
	<button type="button" class="btn btn-{% if outline %}outline-{% endif %}{{type}} {% if sz %}btn-{{sz}} {%endif%}{{cls}}" {{attr | safe}}>{{label}}</button>
{% endmacro %}{# buttons_1 #}

{# CATEGORY: button_group #}

{# XXX:interaccion? alpine? byId? #}
{% macro btn_toolbar(cls='',label='Toolbar with button groups',groups={'Tools': ['save','load']}) %}
	<div class="btn-toolbar {{cls}}" role="toolbar" aria-label="{{label}}">
		{% for lbl,btns in groups %}
			<div class="btn-group{% if not loop.last %} me-2{%endif%}" role="group" aria-label="{{lbl}}">
				{% for lbl in btns %}
					<button type="button" class="btn btn-secondary">{{lbl}}</button>
				{% endfor %}
			</div>
		{% endfor %}
	</div>
{% endmacro %}{# button_group_1 #}

{# CATEGORY: breadcrumb #}

{% macro breadcrumbs(cls='',entries=false, here='Home') %}
	{# SEE: https://www.11ty.dev/docs/plugins/navigation/#example-breadcrumbs #}
	{% if entries==false %}{% set entries= collections.all | eleventyNavigationBreadcrumb(here) %}{% endif %}
	<nav aria-label="breadcrumb {{cls}}">
		<ol class="breadcrumb">
			{% for it in entries %}
				<li class="breadcrumb-item{% if it.url==page.url %}active" aria-current="page{% endif %}"><a href="{{it.url}}">{{it.title}}</a></li>
			{% endfor %}
		</ol>
	</nav>
{% endmacro %}{# breadcrumb_1 #}

{# CATEGORY: alerts #}

{% macro alert(cls='', type='primary',title='',txt='') %}
	<div class="alert alert-{{type}} alert-dismissible fade show {{cls}}" role="alert">
		{% if title!='' %}<h4 class="alert-heading">{{ title }}</h4>{% endif %}
		{% if caller %}{{ caller() }}{% endif %}{{ txt }}
		<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
	</div>
{% endmacro %}

{# CATEGORY: badge #}

{% macro badge(cls='',type='',txt='',rounded=false) %}
	<span class="badge {% if rounded %}rounded-pill{% endif %}bg-{{type}} {{cls}}">{{txt}}</span>
{% endmacro %}{# badge_1 #}

{# CATEGORY: tooltips #}

{% macro tooltip(cls='',where='top',txt='',label='?',type='secondary') %}
	<button type="button" class="btn btn-{{type}} {{cls}}" data-bs-toggle="tooltip" data-bs-placement="{{where}}" data-bs-html="true" title="{{txt}}">{{label}}</button>
{% endmacro %}{# tooltips_1 #}

{# CATEGORY: spinners #}

{% macro wait(cls='',type='primary',label='working') %}
	<div class="spinner-border text-{{type}} {{cls}}" role="status">
		<span class="visually-hidden">{{label}}</span>
	</div>
{% endmacro %}{# spinners_1 #}

{# CATEGORY: progress #}

{% macro progress(cls='',val='0',type='') %}
	<div class="progress {{cls}}">
		<div class="progress-bar" role="progressbar" style="width: {{val}}%" aria-valuenow="{{val}}" aria-valuemin="0" aria-valuemax="100"></div>
		<div class="progress-bar progress-bar-striped progress-bar-animated {%if type%} bg-{{type}}{%endif%}" role="progressbar" style="width: {{ 100 - val }}%" aria-valuenow="{{ 100 - val }}" aria-valuemin="0" aria-valuemax="100"></div>
	</div>
{% endmacro %}{# progress_2 #}

{# CATEGORY: toasts #}

{% macro toasts_1(cls='') %}
	<div class="toast" role="alert" aria-live="assertive" aria-atomic="true">
		<div class="toast-header">
			<svg class="bd-placeholder-img rounded me-2" width="20" height="20" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" preserveAspectRatio="xMidYMid slice" focusable="false"><rect width="100%" height="100%" fill="#007aff"/></svg>
			<strong class="me-auto">Bootstrap</strong>
			<small class="text-muted">11 mins ago</small>
			<button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
		</div>
		<div class="toast-body">
			Hello, world! This is a toast message.
		</div>
	</div>
{% endmacro %}{# toasts_1 #}

{# CATEGORY: tables #}

{# cls in table-dark table-borderless #}
{% macro table(cls='',cols=[],values=[],row_cls_col_idx=-1) %}{# U: cols=list, values=list of lists, put transformations elsewhere #}
	<table class="table table-striped table-hover {{cls}}">
		<thead>
			<tr>
				<th scope="col">#</th>
				{% for col in cols %}
				<th scope="col">{{ col }}</th>
				{% endfor %}
			</tr>
		</thead>
		<tbody>
			{% for v in values %}
			<tr {% if row_cls_col_idx>0 %} class="{{ v[row_cls_col_idx] }}"{% endif %}>
				<th scope="row">{{ loop.index }}</th>
				{% for col in cols %}
				<td>{{ v[loop.index0] }}</td>
				{% endfor %}
			</tr>
			{% endfor %}
		</tbody>
	</table>
{% endmacro %}

