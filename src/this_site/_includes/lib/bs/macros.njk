{# CATEGORY: tables #}

{% macro table(cls='',cols=[],values=[]) %}
	<table class="table table-striped {{cls}}">
		<thead>
			<tr>
				<th scope="col">#</th>
				{% for col in cols %}
				<th scope="col">{{ col }}</th>
				{% endfor %}
			</tr>
		</thead>
		<tbody>
			{% for v in values %}
			<tr>
				<th scope="row">{{ loop.index }}</th>
				{% for col in cols %}
				<td>{{ v[loop.index0] }}</td>
				{% endfor %}
			</tr>
			{% endfor %}
		</tbody>
	</table>
{% endmacro %}

