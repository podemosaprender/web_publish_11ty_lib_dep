{
  "name": "web_publish_11ty_lib_dep",
  "version": "2.1.2",
  "repository": "https://github.com/podemosaprender/web_publish_11ty_lib_dep",
  "description": "A ready to use site builder using 11ty",
  "scripts": {
    "build": "npm run prepare ; eleventy",
    "watch": "eleventy --watch",
    "serve": "eleventy --serve",
    "start": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "prepare": "browserify src/lib/js/search-lunr/common.js --standalone Lunr > src/lib/template/js/lunr.js ; cd src/this_site/; find -type l -exec rm {} \\; ; for i in ../lib/template/* ; do ln -s $i `basename $i`; done"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "eleventy-multisite": "^0.1.1",
    "eleventy-plugin-link_to": "^0.4.1",
    "luxon": "^3.4.4",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.6.7",
    "prettier": "3.3.2"
  },
  "dependencies": {
    "@codegouvfr/eleventy-plugin-calendar": "^3.0.4",
    "browserify": "^17.0.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "lorem-ipsum": "^2.0.8",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.14.0",
    "markdown-it-table-of-contents": "^0.6.0",
    "node-html-parser": "^6.1.13",
    "nunjucks": "^3.2.4"
  }
}
